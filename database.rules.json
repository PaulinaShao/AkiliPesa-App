{
  "rules": {
    "presence": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    },
    "callStatus": {
      "$sessionId": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    },
    "agentLive": {
      "$agentId": {
        ".read": "auth != null",
        ".write": "auth != null && auth.uid == $agentId"
      }
    },
    ".read": false,
    ".write": false
  }
}
